alias cnpm="npm --registry=https://registry.npm.taobao.org \
--cache=$HOME/.npm/.cache/cnpm \
--disturl=https://npm.taobao.org/dist \
--userconfig=$HOME/.cnpmrc"

alias myvps="ssh root@codecore.cc"
alias qzvps="ssh root@133.130.97.101"
alias bu="brew up && brew upgrade && brew cleanup && brew dr"
alias startmongo="mongod -f /usr/local/etc/mongod.conf &"
alias stopmongo="pgrep mongod | xargs kill -9"
alias mongo="mongo --port 6034 -u 'root' -p 'abc123' --authenticationDatabase 'admin'"
alias startmysql="mysql.server start"
alias stopmysql="mysql.server stop"
alias py3="python3"
alias cls="clear"

alias setp="export http_proxy=http://用户名:密码@IP:端口/ https_proxy=http://用户名:密码@IP:端口/ ftp_proxy=http://用户名:密码@IP:端口/"

alias setmp="export http_proxy=http://用户名:密码@IP:端口/ https_proxy=http://用户名:密码@IP:端口/ ftp_proxy=http://用户名:密码@IP:端口/"

alias unsetp="unset http_proxy https_proxy ftp_proxy"

alias cl="curl -i ip.cn"
alias sz="source ~/.zshrc"
alias sd="sudo shutdown -h now"
